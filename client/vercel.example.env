# Vercel Environment Variables for EspacoPessoal Docs
# Copy these variables to your Vercel dashboard Environment Variables section

# Production URLs
NEXT_PUBLIC_NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://docs.espacopessoal.com
NEXT_PUBLIC_PORT=3000

# WebSocket Configuration (IMPORTANT: Use WSS for HTTPS sites)
NEXT_PUBLIC_WS_URL=wss://socket.espacopessoal.com:6001
NEXT_PUBLIC_WS_HOST=socket.espacopessoal.com
NEXT_PUBLIC_WS_PORT=6001

# If your WebSocket server is on a different port or domain, update accordingly:
# NEXT_PUBLIC_WS_URL=wss://your-websocket-server.com:6001

# App Configuration
NEXT_PUBLIC_APP_NAME=EspacoPessoal Docs
NEXT_PUBLIC_MAX_USERS=10
NEXT_PUBLIC_DOCUMENT_NAME=shared-document

# Feature Flags
NEXT_PUBLIC_ENABLE_COLLABORATION=true
NEXT_PUBLIC_ENABLE_PERSISTENCE=true
NEXT_PUBLIC_ENABLE_VOICE_CHAT=false

# Analytics (optional)
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_HOTJAR_ID=xxxxxxx 